    // _spi - A configured spi (MSB_FIRST, 15MHz)
    // _numBits - The number of bits chained together
    constructor(_spi, _maxBits) {
        this.spi = _spi;
        this.maxBits = _maxBits;
        this.clear();
    }
    
    // Clears the pixels
    function clear() {
        bitsToWrite = blob();
        for (local x=0; x < maxBits; x++) this.push(0,0,0);
        this.flush();
        
        bitsToWrite = blob();
    }
    
    // adds a pixel to the buffer
    function push(r,g,b) {
        bitsToWrite.writestring(bits[g]);
        bitsToWrite.writestring(bits[r]);
        bitsToWrite.writestring(bits[b]);
    }
    
    // pushes buffer to pixel (and clears buffer)
    function flush() {
        spi.write(bitsToWrite);
        bitsToWrite = blob();
    }
}

